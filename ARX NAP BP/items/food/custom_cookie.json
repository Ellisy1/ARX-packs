//ДАННЫЙ КОД НЕ ЯВЛЯЕТСЯ ОБРАЗЦОВЫМ!!!
{
  "format_version": "1.16.100",
  "minecraft:item": {
    "description": {
      "identifier": "arx:custom_cookie",
      "category": "equipment"
    },
    "components": {
      "minecraft:creative_category": {
        "parent": "itemGroup.name.cookedFood"
      },
      "minecraft:icon": {
        "texture": "arx:custom_cookie"
      },
      "minecraft:cooldown": {
        "category": "food",
        "duration": 1
      },
      "minecraft:on_use": {
        "on_use": {
          "event": "on_eat"
        }
      },
      "minecraft:fuel": {
        "duration": 3.0
      },
      "minecraft:max_stack_size": 1
    },
    "events": {
      "on_eat": {
        "run_command": {
          "command": [
            "tag @p add tt_f_meat",
            "tag @p add is_solid_food",
            "scoreboard players set @p t_f_deliciousness 42",

            "scoreboard players set @p t_f_sour 50",

            "function food/food"
          ]
        }
      }
    }
  }
}