{
  "format_version": "1.10",
  "minecraft:item": {
    "description": {
      "identifier": "arx:bread"
    },

    "components": {
      "minecraft:creative_category": {
        "parent": "itemGroup.name.cookedFood"
      },
      "minecraft:icon": {
        "texture": "arx:bread"
      },
      "minecraft:food": {
        "saturation_modifier": "normal",
        "nutrition": 2,
        "can_always_eat": false,
        "on_consume": {
          "event": "on_eat"
        }
      },
      "minecraft:use_duration": 1.5,
      "minecraft:use_animation": "eat",
      "minecraft:fuel": {
        "duration": 3.0
      },
      "minecraft:max_stack_size": 1
    },
    "events": {
      "on_eat": {
        "run_command": {
          "command": [
            "tag @p add tt_f_meat",
            "scoreboard players set @p t_f_deliciousness 15",

            "scoreboard players set @p t_f_salty 15",
            "scoreboard players set @p t_f_exotic 25",

            "scoreboard players set @p t_f_poisoning 150",
            "function food/food"
          ]
        }
      }
    }
  }
}