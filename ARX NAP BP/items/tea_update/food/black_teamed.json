{
    "format_version": "1.16.100",
    "minecraft:item": {
      "description": {
        "identifier": "arx:black_tea_med",
        "category": "equipment"
      },
      "components": {
        "minecraft:creative_category": {
          "parent": "itemGroup.name.cookedFood"
        },
        "minecraft:icon": {
          "texture": "arx:black_tea_med"
        },
        "minecraft:cooldown": {
          "category": "food",
          "duration": 1
        },
        "minecraft:on_use": {
          "on_use": {
            "event": "on_eat"
          }
        },
        "minecraft:fuel": {
          "duration": 3.0
        },
        "minecraft:max_stack_size": 1
      },
      "events": {
        "on_eat": {
          "run_command": {
            "command": [
              "tag @p add tt_f_herbal",
              "tag @p add is_liquid_food",
              "scoreboard players set @p t_f_deliciousness 35",
  
              "scoreboard players set @p t_f_sweet 80",
  
              "scoreboard players set @p t_f_poisoning 100",
              "function food/food",
  
              "clear @s[scores={saturation=0}] arx:black_tea_med 0 1",
              "give @s[scores={saturation=0}] arx:cup",
              "scoreboard players set @s[scores={saturation=0}] saturation 120"
            ]
          }
        }
      }
    }
  }