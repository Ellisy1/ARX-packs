						{"sleeping": "query.is_sleeping && query.is_alive"},
						{"attack.positions": "variable.attack_time >= 0.0"},
						{"attack.rotations": "variable.attack_time >= 0.0 && !query.is_item_name_any('slot.weapon.mainhand', 0, 'arx:staff_wooden') && !query.is_item_name_any('slot.weapon.mainhand', 0, 'arx:costorub')"},
