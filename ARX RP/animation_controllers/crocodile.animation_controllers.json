{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.crocodile.basic": {
			"initial_state": "stay",
			"states": {
				"stay": {
					"animations": [
						"stay",
						{"attack": "variable.attack_time > 0"},
						"blink"
					],
					"transitions": [
						{"walking": "q.ground_speed > 1.0"}
					],
					"blend_transition": 0.4
				},
				"walking": {
					"animations": [
						"walk",
						{"attack": "variable.attack_time > 0"},
						"blink"
					],
					"transitions": [
						{"stay": "q.ground_speed < 0.5"}
					],
					"blend_transition": 0.4
				}
			}
		}
	}
}