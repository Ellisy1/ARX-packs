{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.deer.basic": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": [
						"basic",
						{
							"attack": "variable.attack_time > 0.0"
						}
					],
					"transitions": [
						{
							"walking": "q.ground_speed > 1.0"
						}
					],
					"blend_transition": 0.2
				},
				"walking": {
					"animations": [
						"basic",
						"walk",
						{
							"attack": "variable.attack_time > 0.0"
						}
					],
					"transitions": [
						{
							"idle": "q.ground_speed < 0.5"
						}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}