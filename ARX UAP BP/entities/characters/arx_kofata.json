{
  "format_version": "1.10.0",
  "minecraft:entity": {
    "description": {
      "identifier": "arx:kofata",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {},
    "components": {
      "minecraft:type_family": {
        "family": [
          "npc"
        ]
      },
      "minecraft:health": {
        "value": 20,
        "max": 20
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:movement": {
        "value": 0.0
      },
      "minecraft:navigation.walk": {
        "is_amphibious": false,
        "can_path_over_water": false,
        "can_pass_doors": false,
        "can_open_doors": false,
        "can_walk": false,
        "avoid_water": false
      },
      "minecraft:annotation.open_door": {},
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:damage_sensor": [
        {
          "on_damage": {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "lightning"
            },
            "event": "become_witch"
          },
          "deals_damage": false
        },
        {
          "on_damage": {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "zombie"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "husk"
                }
              ],
              "all_of": [
                {
                  "test": "has_damage",
                  "value": "fatal"
                }
              ]
            },
            "event": "become_zombie"
          }
        }
      ],
      "minecraft:behavior.move_indoors": {
        "priority": 4,
        "speed_multiplier": 0.8
      },
      "minecraft:behavior.restrict_open_door": {
        "priority": 5
      },
      "minecraft:behavior.move_towards_restriction": {
        "priority": 7,
        "speed_multiplier": 0.6
      },
      "minecraft:behavior.share_items": {
        "priority": 8,
        "max_dist": 3,
        "goal_radius": 2.0,
        "speed_multiplier": 0.5,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "villager"
            }
          }
        ]
      },
      "minecraft:physics": {
        "has_gravity": true
      },
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 0 : 0"
      },
      "minecraft:underwater_movement": {
        "value": 0.02
      },
      "minecraft:scale": {
        "value": 1
      },
      "minecraft:equip_item": {}
    },
    "events": {},
    "minecraft:entity_transformed": {
      "sequence": [
        {
          "filters": {
            "test": "has_component",
            "subject": "other",
            "value": "minecraft:is_baby"
          },
          "add": {
            "component_groups": [
              "baby"
            ]
          }
        },
        {
          "filters": {
            "test": "has_component",
            "subject": "other",
            "operator": "!=",
            "value": "minecraft:is_baby"
          },
          "add": {
            "component_groups": [
              "adult"
            ]
          }
        }
      ]
    },
    "minecraft:entity_born": {
      "sequence": [
        {
          "add": {
            "component_groups": [
              "baby"
            ]
          }
        }
      ]
    },
    "minecraft:ageable_grow_up": {
      "sequence": [
        {
          "remove": {
            "component_groups": [
              "baby"
            ]
          },
          "add": {
            "component_groups": [
              "adult"
            ]
          }
        }
      ]
    },
    "on:death": {
      "run_command": {
        "command": []
      }
    },
    "on:hurt": {
      "run_command": {
        "command": []
      }
    },
    "on:hurt_by_player": {
      "run_command": {
        "command": []
      }
    },
    "on:ignite": {
      "run_command": {
        "command": []
      }
    },
    "on:target_acquired": {
      "run_command": {
        "command": []
      }
    },
    "on:target_escape": {
      "run_command": {
        "command": []
      }
    },
    "on:wake_with_owner": {
      "run_command": {
        "command": []
      }
    }
  }
}