{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.rat_monstr.walk": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": [
						"stay",
						{"attack": "variable.attack_time > 0.0"}
					],
					"transitions": [
						{"walking": "q.ground_speed > 1.0"}
					],
					"blend_transition": 0.4
				},
				"walking": {
					"animations": [
						"walk",
						"stay",
						{"attack": "variable.attack_time > 0.0"}
					],
					"transitions": [
						{"idle": "q.ground_speed < 0.1"},
						{"running": "query.ground_speed >= 5.5"}
					],
					"blend_transition": 0.4
				},
				"running": {
					"animations": [
						"run_attack",
						"stay",
						{"attack": "variable.attack_time > 0.0"}
					],
					"transitions": [
						{"walking": "query.ground_speed < 5.5"}
					],
					"blend_transition": 0.4
				}
			}
		}
	}
}